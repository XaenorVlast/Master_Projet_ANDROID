<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="fr.isen.gomez.untilfailure.MyApp"
    android:allowBackup="true"
    android:dataExtractionRules="@xml/data_extraction_rules"
    android:fullBackupContent="@xml/backup_rules"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.UntilFailure"
    tools:targetApi="31">

    <activity
        android:name=".model.screenPrincipal.SeanceActivity"
        android:exported="false"
        android:label="@string/title_activity_seance"
        android:theme="@style/Theme.UntilFailure" />
    <activity
        android:name=".model.screenPrincipal.ProfileUserActivity"
        android:exported="false"
        android:label="@string/title_activity_profile_user"
        android:theme="@style/Theme.UntilFailure" />
    <activity
        android:name=".model.firstPart.InscriptionActivity"
        android:exported="false"
        android:label="@string/title_activity_inscription"
        android:theme="@style/Theme.UntilFailure" />
    <activity
        android:name=".model.screenPrincipal.DataPerformancesActivity"
        android:exported="false"
        android:label="@string/title_activity_data_performances"
        android:theme="@style/Theme.UntilFailure" />
    <activity
        android:name=".model.firstPart.ConnexionActivity"
        android:exported="false"
        android:label="@string/title_activity_connexion"
        android:theme="@style/Theme.UntilFailure" />

    <activity
        android:name=".model.screenPrincipal.SettingsUserActivity"
        android:exported="false"
        android:theme="@style/Theme.UntilFailure" />

    <meta-data
        android:name="com.google.firebase.database.DEFAULT_URL"
        android:value="https://untilfailure-ca9de-default-rtdb.europe-west1.firebasedatabase.app/" />

    <activity
        android:name=".model.firstPart.AccueilActivity"
        android:exported="true"
        android:label="@string/title_activity_accueil"
        android:theme="@style/Theme.UntilFailure">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
</application>

    </manifest>
